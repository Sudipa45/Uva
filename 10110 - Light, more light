#include<bits/stdc++.h>
using namespace std;
int main()
{
    long long n;
   // freopen("input.txt", "r", stdin);
    //freopen("output.txt", "w", stdout);
    //cout<<v[v.size()-1]<<endl;
    while(cin>>n)
    {
        if(n==0)break;
        long double t1=sqrt(n);
        if(t1==int(t1))
cout<<"yes"<<endl;
else cout<<"no"<<endl;
}


    return 0;
}
