#include<bits/stdc++.h>
using namespace std;
int main()
{
    int rem,sum=0;
    long long int n;
    while(cin>>n)
    {

        if(n==0)break;
        while(n/10!=0)
        {
            sum=0;
            while(n!=0)
            {
                rem=n%10;
                sum+=rem;
                n=n/10;
            }
            n=sum;
        }
        cout<<n<<endl;
    }





    return 0;
}
